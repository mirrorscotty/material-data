# Annoying global variable definitions. For now, all the model parameters are
# hard coded into the dll using these variables. This should be changed to
# allow them to be read in from a data file.

MW_wat = 18 # Molecular weight of water
MW_pro = 2.6148e5 # Average molecular weight of solids
MW_fat = 2e5
MW_car = 2e5
MW_fib = 2e5
MW_ash = 2e5

Mwat = .9966 # Initial mole fraction of water
Mpro = .0024 # Mole fraction of solids
Mfat = .0001
Mcar = .0002
Mfib = .0003
Mash = .0004
Mice = 0 # Initial mass fraction of ice

# Rate of reaction parameters
Ea = 10000 # Activation Energy (J/mol)
A = 1 # k0 value (1/s)

To = 280 # Initial Temperature
Tf = 273.15 # Freezing point of pure water

L = 6010 # J/mol Latent heat of fusion of water
R = 8.3145 # J/(mol K)

